    <script>

        //动态加载课程信息
        function loadCourseInfo() {

            $.ajax({
                url: "/Home/GetNewCourse?size=6",
                method: "post",
                async: false,
                success: function (data) {

                    if (data.status == "0") {
                        //alert(data.status);
                        //将数据填入相应的图片位置

                        //最新课程
                        var newList = data.newCourseList;
                        insert(newList,"newImg","new");

                        //公选课程
                        var gxList = data.gxCourseList;
                        insert(gxList, "gxImg","gx");

                        //公共必修课
                        var gbList = data.gbCourseList;
                        insert(gbList, "gbImg","gb");

                        //专选课程
                        var zxList = data.zxCourseList;
                        insert(zxList, "zxImg","zx");

                        //专业必修课
                        var zbList = data.zbCourseList;
                        insert(zbList, "zbImg","zb");


                    }
                    else {
                        alert("网络出错啦！！");
                    }
                },
                error: function () {
                    alert("网络出错啦！！");
                }
            });
        }

        //执行插入
        function insert(array, imgId, divId) {

            var rootImg = "/Content/courseImg/";

            var links = document.getElementById(divId).getElementsByTagName("a");

            $.each(array, function (i) {

                links[i+1].innerHTML = array[i].course_name;
                document.getElementById(imgId + (i + 1) + "").src = rootImg + array[i].course_photo;

            });


        }

              //显示新闻详情
        function showDetail(index) {
          
              window.location.href = "/News/ShowDetail?id=" + index;

        }

        function showMore(){
            window.location.href = "/News/ShowMore" 
        }
    </script>
